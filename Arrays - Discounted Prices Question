let price = [250,645,300,900,50];
for(let i =0 ;i<price.length;i++){
    console.log(`Price before Discount: ${price[i]}`);
    price[i] = price[i] * 0.9;
    console.log(`Price after 10% Discount: ${price[i]}`);

}
console.log(`Discounted Prices are: ${price}`);
for(let i of price){
    console.log(i);
}
