let day = "Sunday";

switch(day){
    case "Monday","Tuesday":
        console.log("Workday MAX");
        break;
        
        
    case "Wednesday","Thursday":
        console.log("Workday Managable");
        break;   

    case "Friday":
        console.log("Kal chutti hai");
        break; 
    
        
    case "Saturday","Sunday":
        console.log("Yeahhhhhh Chutti!!! Chutti!!!");
        break;
    default:
        console.log("Enter valid data");    

};
